# Drupal CMS Project .gitignore

# Ignore core and vendor directories
/vendor/
/recipes/
/web/

# Ignore composer files
composer.lock

# Ignore temporary files
*.tmp
*.swp
*.swo
*~

# Ignore OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Ignore IDE files
.vscode/
.idea/
*.sublime-*

# Ignore log files
*.log
/logs/

# Ignore backup files
*.bak
*.backup

# Ignore Drush backup files
*.sql.gz

# Ignore build artifacts
/build/
/dist/

# Ignore node modules and build files
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore ddev generated files
/.ddev/.webimageBuild*
/.ddev/db_snapshots/
